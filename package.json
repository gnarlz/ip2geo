{
  "name": "ip2geo",
  "version": "1.0.0",
  "description": "ip2geo is an IP Address Geolocation API",
  "repository": {
    "type": "git",
    "url": "https://github.com/gnarlz/ip2geo.git"
  },
  "main": "handler.js",
  "scripts": {
    "lint": "standard --verbose | snazzy",
    "start-redis": "sh test/redis-local.sh",
    "stop-redis": "docker stop ip2geo-redis",
    "clean-redis": "docker stop ip2geo-redis && docker rm ip2geo-redis",
    "start-postgres": "sh test/postgres-local.sh",
    "stop-postgres": "docker stop ip2geo-postgres",
    "clean-postgres": "docker stop ip2geo-postgres && docker rm ip2geo-postgres",
    "unit": "NODE_CONFIG_STRICT_MODE=1 NODE_ENV=unit nyc mocha --exit --timeout=1000000 'test/unit/**/*-spec.js'",
    "dev-int": "if test \"$HOME\" != \"/home/appuser\" ; then npm run start-redis ; npm run start-postgres; fi && NODE_CONFIG_STRICT_MODE=1 NODE_ENV=dev-int mocha --exit --timeout=1000000 'test/integration/**/*-spec.js'",
    "int": "NODE_CONFIG_STRICT_MODE=1 NODE_ENV=int mocha --exit --timeout=1000000 'test/integration/**/*-spec.js'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "async": "3.2.1",
    "aws-sdk": "2.995.0",
    "big-integer": "1.6.48",
    "bluebird": "3.7.2",
    "http-codes": "1.0.0",
    "ip-to-int": "^0.3.1",
    "is-ip": "3.1.0",
    "lodash.clonedeep": "4.5.0",
    "lodash.findkey": "4.6.0",
    "lodash.flatten": "4.4.0",
    "lodash.get": "4.4.2",
    "lodash.isempty": "4.4.0",
    "lodash.isequal": "4.5.0",
    "lodash.keys": "4.2.0",
    "lodash.partial": "4.2.1",
    "lodash.range": "3.2.0",
    "lodash.set": "4.3.2",
    "lodash.sortby": "4.7.0",
    "lodash.uniq": "4.5.0",
    "lodash.uniqby": "4.7.0",
    "lodash.unset": "4.5.2",
    "moment": "2.29.1",
    "pg": "8.7.1",
    "postmark": "2.7.8",
    "rate-limiter-flexible": "2.2.4",
    "redis": "3.1.2",
    "stripe": "^7.15.0",
    "uuid": "3.4.0",
    "uuid-validate": "0.0.3",
    "winston": "3.3.3",
    "xml2js": "0.4.23",
    "xmldom": "0.6.0"
  },
  "devDependencies": {
    "chai": " 4.3.4",
    "dirty-chai": "2.0.1",
    "mocha": "9.1.1",
    "nodemon": "2.0.12",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "proxyquire": "2.1.3",
    "snazzy": "8.0.0",
    "standard": "16.0.3"
  }
}
